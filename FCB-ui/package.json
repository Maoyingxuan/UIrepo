{
  "name": "fcb-ui",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "author": "maomao",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maoyingxuan/UIrepo"
  },
  "main": "dist/fcb-ui.cjs.js",
  "module": "dist/fcb-ui.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fcb-ui.es.js",
      "require": "./dist/fcb-ui.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": " vite build &&tsc -b",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "sk": "pnpm run storybook",
    "sk-docs": "storybook dev --docs",
    "test": "vitest",
    "build-storybook-docs": "storybook build --docs"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0",
    "@griffel/react": "^1.5.30",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.2.4",
    "@storybook/addon-interactions": "^8.2.4",
    "@storybook/addon-links": "^8.2.4",
    "@storybook/addon-onboarding": "^8.2.4",
    "@storybook/blocks": "^8.2.4",
    "@storybook/react": "^8.2.4",
    "@storybook/react-vite": "^8.2.4",
    "@storybook/test": "^8.2.4",
    "@storybook/types": "^8.6.14",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "codesandbox-import-utils": "^2.2.3",
    "dedent": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.1.1",
    "storybook": "^8.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-dts": "4.0.0-beta.2",
    "vitest": "^2.0.5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
